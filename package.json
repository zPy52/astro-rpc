{
  "name": "astro-rpc",
  "version": "1.0.4",
  "type": "module",
  "description": "RPC library for the Astro framework that leverages POST APIRoutes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build --declaration && tsc-alias",
    "prepare": "tsc --build --declaration && tsc-alias"
  },
  "keywords": [
    "astro",
    "rpc",
    "api",
    "endpoints"
  ],
  "author": "Antonio Peña Peña",
  "license": "Apache-2.0",
  "homepage": "https://github.com/zPy52/astro-rpc",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "astro": "^5.12.6",
    "react": "^19.1.1",
    "zod": "^3.25.76"
  }
}
